@model List<MiddleWare.Models.Employee>
@{
	ViewData["Title"] = "Employee List";
}
@{
}

<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Name</th>
			<th>place</th>
			
		</tr>
	</thead>
	<tbody>
		@foreach (var employee in Model)
		{
			<tr>
				<td>@employee.Id</td>
				<td>@employee.Name</td>
				<td>@employee.place</td>
				
			</tr>
		}
	
</table>